{
    "name": "ohmybrew/basic-shopify-resource",
    "type": "library",
    "description": "A basic resource wrapper for Basic-Shopify-API",
    "keywords": ["api", "shopify", "active-resource"],
    "license": "MIT",
    "authors": [
      {
        "name": "Tyler King",
        "email": "tyler.n.king@gmail.com"
      }
    ],
    "require": {
        "ohmybrew/basic-shopify-api": "^3.0",
        "tightenco/collect": "^5.7"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.2",
      "phpdocumentor/phpdocumentor": "2.*",
      "satooshi/php-coveralls": "~1.0",
      "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
      "psr-4": { "OhMyBrew\\BasicShopifyResource\\" : "src/OhMyBrew/BasicShopifyResource" }
